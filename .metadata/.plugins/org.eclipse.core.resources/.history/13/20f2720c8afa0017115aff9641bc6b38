package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
    int a [] = { 45, 81, 9, 72, 4, 6};
    int i,j;
    for(i=0 ; i<a.length-1 ; i++){
    	for( j=0 ;j < a.length-1 ; j++){
    		if(a[j+1] < a[j]){
    			int tump = a[j];
    			a[j] = a[j+1];
    			a[j+1]=tump;
    			
    		}
    	}
    }
    for(i=0;i<a.length;i++)
    	System.out.print(a[i]+" ");
	}

}
